{
  "env": {
    "MCP_TIMEOUT": "5000",
    "MCP_TOOL_TIMEOUT": "180000"
  },
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git remote:*)",
      "Bash(git mv:*)",
      "Bash(git rm:*)",
      "Bash(git status:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(open:*)",
      "Bash(rg:*)",
      "Bash(gh search code:*)",
      "Bash(gh api:*)",
      "Bash(python3:*)",
      "Bash(gradle:*)",
      "Bash(java:*)",
      "Bash(jar:*)",
      "Bash(unzip:*)",
      "Bash(javap:*)",
      "Bash(find:*)",
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:docs.github.com)",
      "WebFetch(domain:api.github.com)",
      "WebFetch(domain:www.jetbrains.com)",
      "WebFetch(domain:rider-support.jetbrains.com)",
      "WebFetch(domain:intellij-support.jetbrains.com)",
      "WebFetch(domain:youtrack.jetbrains.com)",
      "WebFetch(domain:plugins.jetbrains.com)",
      "WebFetch(domain:modelcontextprotocol.info)",
      "WebFetch(domain:medium.com)",
      "mcp__jetbrains"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm:*)",
      "Bash(rm -rf:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(nc:*)",
      "Bash(npm publish:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git commit --amend:*)",
      "Bash(git push -f:*)",
      "Bash(git reset:*)",
      "Bash(git rebase:*)",
      "Bash(git switch:*)",
      "Bash(psql:*)",
      "Bash(mysql:*)",
      "Bash(mongod:*)",
      "Read(.env.*)",
      "Read(id_rsa)",
      "Read(id_ed25519)",
      "Read(**/*token*)",
      "Read(**/*key*)",
      "Read(!**/*monkey*)",
      "Read(./Library/**)",
      "Read(!./Library/PackageCache/**)",
      "Edit(**/packages-lock.json)",
      "Write(.env*)",
      "Write(**/secrets/**)"
    ],
    "defaultMode": "plan",
    "additionalDirectories": []
  },
  "enabledMcpjsonServers": [
    "jetbrains"
  ],
  "enabledPlugins": {
    "csharp-lsp@claude-plugins-official": true
  }
}
